@using TravelToLearn.Models.Siniflar
@model List<Blog>
<div class="padding">
    <h4>En İyi 10 Şehir</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var x in Model)
        {
            <li class="list-bg"><span>@{i++; } @i.</span><a href="/Blog/SehirDetay/@x.ID">@x.Baslik</a></li>
        }
    </ul>
    <a href="/Blog/Index" class="link1">Bütün Şehirleri Gör..</a>
</div>
